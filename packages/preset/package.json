{
  "name": "frontiers-primevue-preset",
  "version": "1.0.0",
  "description": "Frontiers custom preset for PrimeVue, based on Lara preset",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm exec tsdown",
    "dev": "pnpm exec tsdown --watch",
    "prepublishOnly": "pnpm build",
    "dev:link": "pnpm link --global && npm link"
  },
  "keywords": [
    "primevue",
    "preset",
    "frontiers"
  ],
  "author": "Alexis Reina",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@primeuix/themes": "catalog:"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "tsdown": "^0.14.2"
  }
}
