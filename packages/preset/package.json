{
  "name": "frontiers-primevue-preset",
  "version": "1.0.0",
  "description": "Frontiers custom preset for PrimeVue, based on Lara preset",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm exec tsc",
    "dev": "pnpm exec tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "pnpm clean",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "primevue",
    "preset",
    "frontiers"
  ],
  "author": "Alexis Reina",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "peerDependencies": {
    "@primeuix/themes": ">=1.0.0"
  },
  "devDependencies": {
    "@primeuix/themes": "catalog:",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}
